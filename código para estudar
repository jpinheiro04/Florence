stegh.sh

#!/bin/bash

path="/home/kali/forense/Caso_Steg/evidencias"

# Loop para percorrer todos os "jpg"
for arquivo in "$path"/*.jpg; do
	if [ -f "$arquivo" ]; then
	   echo "Processando arquivo: $arquivo"
	   for password in `cat wordlist.txt` do
		steghide extract -sf $arquivo -p $password > /dev/null 2>&1
	
	       if [$? -eq 0 ]; then
		echo "Algo foi encontrado."
		echo "." 	
		fi	   
	   done
       fi
done


touch wordlist.txr
